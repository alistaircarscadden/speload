class Level
{
  final int BLOCK_WIDTH = 30;
  Block[][] world = new Block[20][20];
  
  Level()
  {
    this.world = this.generate();
  }
  
  Block[][] generate()
  {
    lvl = level.levelNumber;
    Block[][] temp = new Block[20][20];
    
    for(int i = 0; i < 20; ++i)
    {
      for(int ii = 0; ii < 20; ++ii)
      {
        temp[i][ii] = 1;
        
        if((int)random(30-lvl) == 0)
          temp[i][ii] = 2;
      }
    }
    return temp;
  }
}
